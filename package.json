{
  "name": "celestial-gate",
  "version": "1.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "export NODE_ENV=development && nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "export NODE_ENV=production && node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "db:migrate:dev": "export NODE_ENV=development && npx ts-node scripts/migrations/pg-migrate",
    "db:migrate:undo:dev": "export NODE_ENV=development && npx ts-node scripts/migrations/pg-migrate-undo",
    "db:seed:dev": "set NODE_ENV=development && npx ts-node scripts/migrations/pg-seed-all",
    "db:migrate:prod": "export NODE_ENV=production && npx ts-node scripts/migrations/pg-migrate",
    "db:migrate:undo:prod": "export NODE_ENV=production && npx ts-node scripts/migrations/pg-migrate-undo",
    "db:seed:prod": "set NODE_ENV=production && npx ts-node scripts/migrations/pg-seed-all",
    "db:re-init": "export NODE_ENV=development && npm run db:migrate:undo:dev && npm run db:migrate:dev && npm run db:seed:dev",
    "db:init": "export NODE_ENV=development && npm run db:migrate:dev && npm run db:seed:dev"
  },
  "dependencies": {
    "@nestjs-modules/mailer": "^1.9.1",
    "@nestjs/axios": "^3.0.1",
    "@nestjs/cache-manager": "^2.1.1",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.1.1",
    "@nestjs/core": "^10.0.0",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/sequelize": "^10.0.0",
    "@supabase/supabase-js": "^2.39.1",
    "axios": "^1.6.2",
    "bcrypt": "^5.1.1",
    "cache-manager": "^5.3.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "file-type": "^16.5.4",
    "filesize": "^10.1.0",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "nanoid": "^3.3.7",
    "nodemailer": "^6.9.7",
    "pg": "^8.11.3",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "sequelize": "^6.35.2",
    "sequelize-typescript": "^2.1.6"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@types/bcrypt": "^5.0.2",
    "@types/express": "^4.17.17",
    "@types/file-type": "^10.9.1",
    "@types/jest": "^29.5.2",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.3.1",
    "@types/sequelize": "^4.28.19",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.59.11",
    "@typescript-eslint/parser": "^5.59.11",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.5.0",
    "prettier": "^2.8.8",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  }
}
